<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - electionAnalytics</title>

    <environment exclude="Development">
        <link href="~/css/style.css" rel="stylesheet"/>
        <link href="~/css/c3min.css" rel="stylesheet" />
        <link href="~/css/morris.css" rel="stylesheet"/>
        <link href="~/css/cssChart.css" rel="stylesheet"/>
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet"/>
        <link href="https://fonts.googleapis.com/css?family=Satisfy" rel="stylesheet">
        <link href="https://fonts.googleapis.com/css?family=Merienda" rel="stylesheet">
    </environment>
    
</head>
<body>
    <div class="main-wrapper" id="main-wrapper">

        <div class="preloader">
            <div class="loader">
                <div class="loader__figure"></div>
            </div>
        </div>
         <header class="topbar">
            <nav class="blue-grey darken-3">
                <div class="wrapper">
                    <ul class="brand-logo"><i></i><i></i><i></i><i></i><i></i>
                        <li>
                            <span class="blue-grey darken-3" style="font-family: 'Satisfy', cursive; font-size: 20px;">ChartiFy</span>
                        </li>
                    </ul>
                    <ul class="right">
                        <li>
                            <a class="white-text tooltipped" data-position="left" data-tooltip="Home" asp-action="Index" asp-controller="Home"><i class="material-icons">account_balance</i></a></a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <partial name="_CookieConsentPartial" />
        <div class="section">
            @RenderBody()
        </div>
    </div>
    <environment exclude="Development">
        <script src="~/js/site.js" asp-append-version="true"></script>
        <script src="~/js/jquery.js"></script>
        <script src="~/js/materialize.js"></script>
        <script src="~/js/scrollbar.js"></script>
        <script src="~/js/app.js"></script>
        <script src="~/js/init.js"></script>
        <script src="~/js/custom.js"></script>
    </environment>
    @RenderSection("Scripts", required: false)
</body>
</html>
